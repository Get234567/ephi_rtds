            </tr>
        {% else %}
            <tr>
                <td colspan="4">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('study_dataset_link_request_new') }}">Create new</a>
{% endblock %}

{% block javascripts %}
    {{ parent() }}
    	<script src="{{asset('assets/js/select2.min.js')}}"></script>
