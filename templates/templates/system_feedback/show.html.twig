{% extends 'base.html.twig' %}

{% block title %}SystemFeedback{% endblock %}

{% block body %}
    <h1>SystemFeedback</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ system_feedback.id }}</td>
            </tr>
            <tr>
                <th>FullName</th>
                <td>{{ system_feedback.fullName }}</td>
            </tr>
            <tr>
                <th>Email</th>
                <td>{{ system_feedback.email }}</td>
            </tr>
            <tr>
                <th>Title</th>
                <td>{{ system_feedback.title }}</td>
            </tr>
            <tr>
                <th>Body</th>
                <td>{{ system_feedback.body }}</td>
            </tr>
            <tr>
                <th>SubmittedDate</th>
                <td>{{ system_feedback.submittedDate ? system_feedback.submittedDate|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>Status</th>
                <td>{{ system_feedback.status }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('system_feedback_index') }}">back to list</a>

    <a href="{{ path('system_feedback_edit', {'id': system_feedback.id}) }}">edit</a>

    {{ include('system_feedback/_delete_form.html.twig') }}
{% endblock %}
