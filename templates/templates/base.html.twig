                dataType: "json",
                success: function(msg){
					var total=0;
					var up_count=0;
					var down_count=0

					 msg.forEach(function(element)
					{

						  if(element.RequestType=="Upload")
						  {
							    $("#uploads").html(element.count);
								total=total + parseInt(element.count);
								$("#uploads_container").show();
								up_count=1;
						  }
						 if(element.RequestType=="Download")
						  {
								$("#downloads").html(element.count);
								total=total + parseInt(element.count);
								$("#downloads_container").show();
								down_count=1;
						  }
					});
					if(up_count == 0)
					{
						$("#uploads_container").hide();
					}
					if(down_count == 0)
					{
						$("#downloads_container").hide();
					}





