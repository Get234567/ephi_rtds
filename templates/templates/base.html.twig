						$("#signup_counter_container").show();
					}
					else
					{
						$("#signup_counter_container").hide();
					}




					$.ajax({
					type: "GET",
					url: "{{path('feedback_show')}}",
					dataType: "json",
					success: function(msg3){
					msg3.forEach(function(element3)
					{
						total=total + parseInt(element3.count);
						if(parseInt(element3.count)>0)
						{

							$("#feedback").html(element3.count);
							$("#feedback_container").show();

						}
					   else
						{
							$("#feedback_container").hide();
						}
						if(total > 0)
						{
							$("#total").html("<i> "+total + " Notifications </i>");
							$("#message").html(total);

							$("#message_container").show();
							$("#total_container").show();
						}
					   else
						{
							$("#total_container").hide();
							$("#message_container").hide();
						}

					});

					}
					});


                }
            });






                }//end of success
            });
            return false;
        }

		</script>

