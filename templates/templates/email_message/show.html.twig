{% extends 'base.html.twig' %}

{% block title %}EmailMessage{% endblock %}

{% block body %}
    <h1>EmailMessage</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ email_message.id }}</td>
            </tr>
            <tr>
                <th>Message</th>
                <td>{{ email_message.message }}</td>
