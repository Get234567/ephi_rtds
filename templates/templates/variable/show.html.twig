{% extends 'base.html.twig' %}

{% block title %}Variable{% endblock %}

{% block body %}
    <h1>Variable</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ variable.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ variable.name }}</td>
            </tr>
            <tr>
                <th>LabelDescription</th>
                <td>{{ variable.labelDescription }}</td>
            </tr>
            <tr>
                <th>ValueCode</th>
                <td>{{ variable.valueCode }}</td>
            </tr>
            <tr>
                <th>ValueLevel</th>
                <td>{{ variable.valueLevel }}</td>
            </tr>
            <tr>
                <th>RangeOfValue</th>
                <td>{{ variable.rangeOfValue }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('variable_index') }}">back to list</a>

    <a href="{{ path('variable_edit', {'id': variable.id}) }}">edit</a>

    {{ include('variable/_delete_form.html.twig') }}
{% endblock %}
