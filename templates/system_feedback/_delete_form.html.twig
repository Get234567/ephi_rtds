<form method="post" action="{{ path('system_feedback_delete', {'id': system_feedback.id}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ system_feedback.id) }}">
    <button class="btn">Delete</button>
</form>
